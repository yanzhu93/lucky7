/* pages/home/home.wxss */
.home-content{
  padding: 20rpx;
}
.header{
  margin-top:50rpx;
  height:50rpx;
  line-height: 50rpx;
  text-align: center;
}
.home-body{
  width: 100%;
  margin-top:20rpx;
}
.slide-image{
  width: 100%;
}
.home-nav{
  margin-top:20rpx;
  font-size:14px;
  box-shadow: 0px 2px 2px #ccc;
  height:80rpx;
  line-height: 80rpx;
  padding-left:20rpx;
}
.home-nav text:first-of-type{
  font-size: 18px;
  margin-right:20rpx;
}
.home-item{
  margin-top:30rpx;
}
.home-item view{
  width: 49%;
  height:150rpx;
  background: #ddd;
}
.home-item view:first-of-type{
  float:left;
}
.home-item view:last-of-type{
  float:right;
}